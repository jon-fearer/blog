{
  "name": "site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cp -r ../content/ ./src/assets/content/; ng serve --open",
    "build": "cp -r ../content/ ./src/assets/content/; rm -rf dist; mkdir dist; ng build --progress false --configuration production --output-path dist",
    "test": "jest",
    "lint": "eslint -c .eslintrc.js --max-warnings 1 \"src/**/*.ts\"",
    "lint:fix": "eslint -c .eslintrc.js --fix \"src/**/*.ts\"",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.8",
    "@angular/common": "^15.2.8",
    "@angular/compiler": "^15.2.8",
    "@angular/core": "^15.2.8",
    "@angular/forms": "^15.2.8",
    "@angular/platform-browser": "^15.2.8",
    "@angular/platform-browser-dynamic": "^15.2.8",
    "@angular/router": "^15.2.8",
    "ngx-device-detector": "^5.0.1",
    "primeicons": "^6.0.1",
    "primeng": "^15.4.1",
    "rxjs": "^6.5.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular-devkit/schematics": "^15.2.6",
    "@angular/cli": "^15.2.6",
    "@angular/compiler-cli": "^15.2.8",
    "@angular/language-service": "^15.2.8",
    "@types/jest": "^27.4.0",
    "@types/node": "^8.9.4",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.1",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "serialize-javascript": ">=2.1.1"
  }
}
