{
  "name": "site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cp -r ../content/ ./src/assets/content/; ng serve --open",
    "build": "cp -r ../content/ ./src/assets/content/; rm -rf dist; mkdir dist; ng build --progress false --configuration production --output-path dist",
    "test": "jest",
    "lint": "eslint -c .eslintrc.js --max-warnings 1 \"src/**/*.ts\"",
    "lint:fix": "eslint -c .eslintrc.js --fix \"src/**/*.ts\"",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.2",
    "@angular/common": "^13.2.2",
    "@angular/compiler": "^13.2.2",
    "@angular/core": "^13.2.2",
    "@angular/forms": "^13.2.2",
    "@angular/platform-browser": "^13.2.2",
    "@angular/platform-browser-dynamic": "^13.2.2",
    "@angular/router": "^13.2.2",
    "primeicons": "^4.1.0",
    "primeng": "^13.1.1",
    "rxjs": "^6.5.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.3",
    "@angular-devkit/schematics": "^13.2.3",
    "@angular/cli": "^13.2.3",
    "@angular/compiler-cli": "^13.2.2",
    "@angular/language-service": "^13.2.2",
    "@types/jest": "^26.0.24",
    "@types/node": "^8.9.4",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "serialize-javascript": ">=2.1.1"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ]
  }
}
